pipeline{
  agent any 

  parameters {
    gitParameter(
      branch: '',
      branchFilter: ".*",
      defaultValue: "",
      description: '',
      listSize: '10',
      name: 'Version',
      quickFilterEnabled: false,
      selectedValue: 'NONE',
      sortMode: 'ASCENDING_SMART',
      tagFilter: "*",
      type: 'PT_BRANCH_TAG',
      useRepository: 'git@github.com/lakhanyawa/ansible-basics')
   }
   stages{
      stage ("echo Git Tag") {
        steps {
          echo "${params.Version}"
          echo "${params.COMMIT_ID}"
        }
     }
  } 
}
